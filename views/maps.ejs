<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css'>
    <link rel='stylesheet' href='/bower_components/components-font-awesome/css/font-awesome.min.css'/>
</head>
<body>
<div class="row">
    <div class="well">
        <div class="logo-map"></div>
        <h1 style="margin-left: 120px;"><%= subtitle %></h1>
        <div style="float: right; margin-top: -45px;">
            <ul style="display: inline-block">
                <li style="list-style-type: none"><a class="btn btn-success" href="/locations">Вернуться к списку</a></li>
            </ul>
        </div>
    </div>
</div>
<hr/>

<div id="map_canvas"></div>
<div style="float: right; width: 25%;">
    <div class="col-xs-12">
        <h3 class="text-center">Фильтр</h3>
        <div class="well" style="max-height: 300px;overflow: auto;">

            <select class="form-control" id="type" onchange="filterMarkers(this.value);">
                <option value="">Все устройства</option>
                <% result.forEach(function(res){ %>
                <option value="<%= res.pureId %>"> <%= res.pureId  %> </option>
                <% }); %>
            </select>
        </div>
    </div>
</div>


<script src='http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js'></script>
<!--<script src='/jquery/dist/jquery.min.js'></script>-->
<script src='https://maps.googleapis.com/maps/api/js?&sensor=false'></script>
<script src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js'></script>
<script src='/javascripts/common.js'></script>
<!--<script src='/javascripts/device.js'></script>-->
</body>
</html>
